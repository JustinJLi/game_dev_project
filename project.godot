config_version=5

[application]

config/name="final_project"
run/main_scene="res://Assets/UI/main_menu/main_menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://Assets/Art/bloodsplat3_strip15.png"
config/icon="res://Assets/Art/crosshair.png"

[autoload]

InteractionManager="*res://Assets/Interaction/InteractionManager.tscn"
DialogueManager="*res://Assets/Dialogue/DialogueManager.gd"
Global="*res://Assets/Global/global.gd"
PlayerData="*res://Assets/Player/playerData.gd"
EnemyStats="*res://Assets/Enemy/enemyStats.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/stretch/mode="viewport"
mouse_cursor/custom_image="res://Assets/Art/crosshair.png"
mouse_cursor/custom_image_hotspot=Vector2(0, 10)
mouse_cursor/icon="res://icon.svg"
mouse_cursor/image=""
mouse_cursor/bg_color=Color(0.48, 0.48, 0.48, 1)

[editor_plugins]

enabled=PackedStringArray("res://addons/vision_cone_2d/plugin.cfg")

[global_group]

Environment=""
Player=""
Enemies=""
Hostages=""

[importer_defaults]

animation_library={
"animation/fps": 60.0
}

